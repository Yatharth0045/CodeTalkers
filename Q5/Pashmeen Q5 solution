import java.util.Scanner;
import java.util.HashMap;


public class Main {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int arr[]=new int[n+1];
        arr[0]=1;
        for(int i=1;i<n+1;i++)
        {
            arr[i]=arr[i-1]*i;
        }
        System.out.println(arr[n]);
    }
   
}
